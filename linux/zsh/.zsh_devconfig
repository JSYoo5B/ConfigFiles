#!/bin/zsh

############################################################################
## Common configuration													   #
############################################################################
export PATH_ORG=$PATH
export GCC_COLORS='error=01;31:warning=01;35:note=01;36:caret=-1;32:locus=01:quote=01'

export PATH="$PATH:/usr/local/opt/qt/bin"

# Enable direnv hook
[ -x "$(command -v direnv)" ] && eval "$(direnv hook zsh)"

